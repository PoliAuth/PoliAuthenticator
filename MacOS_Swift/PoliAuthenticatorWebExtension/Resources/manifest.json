{
    "manifest_version": 2,
    "name": "PoliAuthenticator WebExtension",
    "description":"Web extension for PoliAuthenticator",
    "version": "1.0.1",
    "default_locale": "en",
    "content_scripts": [
        {
            "matches":[
                "<all_urls>"
            ],
            "js":[
                "jquery.min.js",
                "content.js"
            ]
        }
    ],
    "permissions": [
        "activeTab",
        "storage"
    ],
    "sandbox": {
        "pages": [
            
        ],
        "content_security_policy":  "sandbox allow-scripts; script-src 'self'"
         
    },
    "browser_action": {
        "default_title": "PoliAuthenticator WebExtension",
        "default_icon": {
            "16": "images/icon16.png",
            "24": "images/icon24.png",
            "32": "images/icon32.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "16": "images/icon16.png",
        "24": "images/icon24.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}

